{
    "manifest_version": 2,
    "name": "YouTube Link Modifier",
    "version": "1.0",
    "description": "Modifies YouTube watch links",
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/watch*"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["webNavigation"]
  }
  